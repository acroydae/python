seq = [1,2,3,4,5]

lambda

sq = lambda x : x ** 2

sq(2)

map(sq,seq)

list(map(sq,seq))

list(map(lambda x : x**2 ,  seq))

list(filter(lambda x:x%2 == 0 , seq))

