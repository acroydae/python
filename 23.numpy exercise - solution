import numpy as np

np.zeros(5)

np.ones(10)

np.arange(10,15)

np.arange(10,50,2)

np.arange(9).reshape(3,3)

np.eye(3)

np.random.rand(1)

np.random.randn(25)

ar100 =np.linspace(1,100,100).reshape(10,10)

ar100

ar100[1,2]

ar100[:1,2]

ar100[1:,2]

ar100[1:,2:]

ar100[1:,2:3]

ar100[1:,2]

ar100.sum()

ar100.mean()

ar100.std()

